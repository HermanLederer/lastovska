{
  "name": "lastovska",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "yarn --cwd ./packages/backend develop",
    "dev:frontend": "yarn --cwd ./packages/frontend dev",
    "dev": "concurrently -n f,b \"yarn dev:frontend\" \"yarn dev:backend\"",
    "build:backend": "yarn --cwd ./packages/backend build",
    "build:frontend": "yarn --cwd ./packages/frontend build",
    "build": "yarn build:frontend & yarn build:backend",
    "start:backend": "yarn --cwd ./packages/backend start",
    "start:frontend": "yarn --cwd ./packages/frontend preview",
    "start": "yarn start:frontend & yarn start:backend"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.5.0",
    "typescript": "^4.5.4"
  }
}
